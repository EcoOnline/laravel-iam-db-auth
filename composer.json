{
    "name": "ecoonline/laravel-iam-db-auth",
    "description": "AWS IAM db auth package for Laravel - EcoOnline fork with security fixes",
    "version": "1.0.0",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Nitin Goyal",
            "email": "nitingoyal.dev@gmail.com"
        }
    ],
    "scripts": {},
    "require": {
        "aws/aws-sdk-php-laravel": "^3.3.0",
        "php": "^7.3|^8.0|^8.1|^8.2|^8.3",
        "ext-pdo": "*"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "EcoOnline\\DBAuth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EcoOnline\\DBAuth\\IamDatabaseConnectorProvider"
            ],
            "aliases": {
            }
        }
    },
    "require-dev": {
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0|^10.0"
    }
}
