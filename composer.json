{
    "name": "ecoonline/laravel-iam-db-auth",
    "description": "AWS IAM db auth package for Laravel - EcoOnline fork with security fixes",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Nitin Goyal",
            "email": "nitingoyal.dev@gmail.com"
        },
        {
            "name": "Konstantinos Papadopoulos",
            "email": "konstantinos.papadopoulos@ecoonline.com"
        }
    ],
    "scripts": {},
    "require": {
        "aws/aws-sdk-php-laravel": "^3.3.0",
        "php": "^8.0|^8.1|^8.2|^8.3",
        "ext-pdo": "*"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "EcoOnline\\DBAuth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EcoOnline\\DBAuth\\IamDatabaseConnectorProvider"
            ],
            "aliases": {
            }
        }
    },
    "require-dev": {
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
    }
}
